example@form page
<%= form_tag("/job_aplication", method: "get") do  %>
  <p>
    <%= label_tag :full_name, "Label String", for: "job_aplication_full_name" %> <br>
    <%= text_field_tag :full_name, "", id: "job_aplication_full_name" %>
  </p>

  <p>
    <%= label_tag :phone %><br>
    <%= telephone_field_tag :phone %>
  </p>

  <p>
    <%= label_tag :hobby %><br>
    <%= text_field_tag :hobby %>
  </p>

  <p>
    <%= label_tag :years_experience %><br>
    <%= number_field_tag :years_experience %>
  </p>

  <p>
    <%= label_tag :available_date %><br>
    <%= date_field_tag :available_date %>
  </p>

  <p>
    <%= submit_tag "commit" %>
  </p>
<% end %>
